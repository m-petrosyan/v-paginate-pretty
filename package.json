{
  "name": "v-paginate-pretty",
  "version": "1.0.2",
  "description": "Beautiful and customizable pagination component for Vue 3 with Composition API",
  "main": "./dist/v-paginate-pretty.umd.js",
  "module": "./dist/v-paginate-pretty.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/v-paginate-pretty.es.js",
      "require": "./dist/v-paginate-pretty.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css",
    "./dist/themes/*": "./dist/themes/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "playground": "cd playground && npm run dev",
    "playground:build": "cd playground && npm run build",
    "deploy": "npm run playground:build && gh-pages -d playground/dist"
  },
  "keywords": [
    "vue",
    "vue3",
    "pagination",
    "component",
    "composition-api",
    "typescript",
    "customizable",
    "themes"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "gh-pages": "^6.3.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^1.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-petrosyan/v-paginate-pretty.git"
  },
  "bugs": {
    "url": "https://github.com/m-petrosyan/v-paginate-pretty/issues"
  },
  "homepage": "https://m-petrosyan.github.io/v-paginate-pretty/"
}
